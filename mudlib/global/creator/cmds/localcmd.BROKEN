#include <standard.h>
#include <cmd.h>
inherit CMD_BASE;

void setup()
{
position = 1;
}



static int cmd(string str, object me) {
string *cmds, *cnames;
int i;

  cnames = ({ });
  cmds = me->query_commands();
  for (i=0;i<sizeof(cmds);i++)
    cnames += ({ cmds[i][0] });
  me->more_string("Currently Defined Commands:\n"+
     sprintf("%#*-s\n", (int)me->query_cols(),
    implode(cnames, "\n")));
  return 1;
} /* local_cmds() */
 
